<h1>メッセージ一覧</h1>

<ul>
  <% @messages.each do |message| %>
    <li><%= link_to message.id, message %> : 
          <%= message.title %> &gt; 
          <%= message.content %></li>
  <% end %>
</ul>
    <!-- karamariを使ってページネーション-->
<%= paginate @messages %>

<%= link_to '新規メッセージの投稿', new_message_path %>
